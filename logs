#искоренить времѧ time



цѣло императоръ() { // русская рулетка
    знакъ test = 'A';

    пути (test) {
        яко 'a':
            молвитьфъ("test == 'a'\n");
            конѣцъ;
        яко 'b':
            молвитьфъ("test == 'b'\n");
            конѣцъ;
        умолчаніе:
            молвитьфъ("test != 'b' && test != 'a'\n");
    }

    дань 0;
}
