#искоренить ЧТѢНІЕ "r"
#искоренить НАПИСАНІЕ "w"
#искоренить БОГОПОДОБНЫЙ "r+"
#искоренить ЧТѢНІЕ_НАРОДНОЕ "rb"
#искоренить НАПИСАНІЕ_НАРОДНОЕ "wb"
#искоренить БОЖЕСТВО "r+b"

#искоренить ОКО_КОНѢЦЪ SEEK_END
#искоренить ОКО_ВНЕМЛИТЬ SEEK_SET





цѣло императоръ(цѣло цѣнность, знакъ** подписи) {
  // Открыть лѣтопись из подписѣй, данных Богом
  // чрѣзъ терминалъ
  ЛѢТОПИСЬ* л = лоткрыть(подписи[1], ЧТѢНІЕ);
  лподмѣнить(л, 0, ОКО_КОНѢЦЪ);
  краткій длина = лглядеть(л);    // Высчитать длину
                                  // лѣтописи
  лподмѣнить(л, 0, ОКО_ВНЕМЛИТЬ); // Вѣрнуться въ начало
                                  // лѣтописи для чтѣнія
  знакъ знать[длина];             // Выдѣлить знать для
                                  // прочтѣнія лѣтописи
  лчитать(знать, 1, длина, л);    // Прочѣсть лѣтопись

//                               /-----\
//                               v     |
  для (краткій с = 0; с < длина; с++) {  // это что за страшидло?
    молвитьзъ(знать[с]);
  }
  молвитьзъ(10);

  лприкрыть(л);                 // Закрыть лѣтопись
  дань 0;
}

