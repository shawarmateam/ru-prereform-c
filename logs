#внѣдрить <time.h>
#искоренить времѧ time



цѣло императоръ() { // комментарий
    пчиркануть(времѧ(НИЧТО));
    
    цѣло random = (чиркануть() % 8)+1;

    коли (random == 8) {
        приказъ("echo \"test\"");
    }
    дань 0;
}
