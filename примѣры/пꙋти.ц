#гвнѣдрить библіѧ/пстъввъ.г
#гвнѣдрить библіѧ/пстъбиблиѧ.г

КЪ0 императоръ() {
    знакъ тест;
    внемлитьфъ("%d", &тест);

    пути (тест) {
        яко 'a':
            молвитьфъ("test == 'a'\n");
            конѣцъ;
        яко 'b':
            молвитьфъ("test == 'b'\n");
            конѣцъ;
        умолчаніе:
            молвитьфъ("test != 'b' && test != 'a'\n");
    }
}

