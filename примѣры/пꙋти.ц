#гвнѣдрить библіѧ/пстъввъ.г
#гвнѣдрить библіѧ/пстъбиблиѧ.г

БЪ0 императоръ() {
    знакъ тест;
    внемлитьфъ("%d", &тест);

    пути (тест) {
        яко 'a':
            молвитьфъ("test == 'a'\н");
            конѣцъ;
        яко 'b':
            молвитьфъ("test == 'b'\н");
            конѣцъ;
        умолчаніе:
            молвитьфъ("test != 'b' && test != 'a'\н");
    }
}

