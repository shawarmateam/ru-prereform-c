#гвнѣдрить писатьвв.г

цѣло императоръ() { // комментарий
    молвитьфъ("слава Петру I\n");
    молвитьфъ("и дворянамъ слава\n");

    знакъ test[100];
    
    молвитьфъ("Введите-c строку: ");
    коли (гвзѧть(test, размеръ(test), писатьвъ) != НИЧТО) {
        молвитьфъ("Вы ввели: %s", test);
    } але {
        молвитьфъ("Курьезъ при чтении строки.\n");
    }

    дань 0;
}
